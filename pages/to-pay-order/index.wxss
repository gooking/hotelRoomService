  font-size: 28rpx;
  color: #FEB21C;
  letter-spacing: 0;
}
.jiesuan-box .left-price .price{
  margin-left: 16rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FFFFFF;
