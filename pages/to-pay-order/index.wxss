@import "/template/login/index.wxss";
page {
  background-color: #f7f8fa;
}
.address{
  width: 100vw;
  height: 148rpx;
  background: #FFFFFF;
  display: flex;
  justify-content: space-between;
  align-items: center;  
}
.address .icon {
  margin-right: 24rpx;
  color: #999999;
}
.address .add {
  width: 702rpx;
  height: 100rpx;
  margin: 24rpx;
  border: 2rpx solid #FEB21C;
  border-radius: 8rpx;
  line-height: 100rpx;
  text-align: center;
}
.address .add image {
  width: 20rpx;
  height: 20rpx;
}
.address .add text {
  margin-left: 16rpx;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #FEB21C;
  letter-spacing: 0;
}
.address .info {
  padding: 32rpx 24rpx;
}
.address .info .a {
  font-family: PingFangSC-Semibold;
  font-size: 30rpx;
  color: #555555;
  letter-spacing: 0;
  line-height: 30rpx;
}
.address .info .b {
  margin-top: 24rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 24rpx;
}

.goods-list {
  width: 100vw;
  background-color: #fff;
  margin-top: 24rpx;
  padding-top: 24rpx;
}
.goods-list .item {
  width: 100vw;
  display: flex;
  margin-top: 24rpx;
}
.goods-list .item .img {
  margin-left: 48rpx;
  width: 152rpx;
  height: 152rpx;
  border-radius: 8rpx;
}
.goods-list .item .profile {
  width: 460rpx;
  display: flex;
  flex-direction: column;
  padding: 20rpx 24rpx;
}
.goods-list .item .profile .name {
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 26rpx;
}
.goods-list .item .profile .guige {
  margin-top: 20rpx;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 20rpx;
}
.goods-list .item .profile .price {
  margin-top: 20rpx;
  display: flex;
  justify-content:space-between;
  align-items:center;
}
.goods-list .item .profile .price .p {
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  color: #FEB21C;
  letter-spacing: 0;
  text-align: right;
  line-height: 26rpx;
}
.goods-list .item .profile .price .n {
  font-size: 26rpx;
  color: #888;
}

.coupon-number {
  margin-top: 24rpx;
  width: 750rpx;
  height: 98rpx;
  line-height: 98rpx;
  background: #FFFFFF;
  display: flex;
  justify-content:space-between;
}
.coupon-number .t {
  margin-left: 24rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #333330;
  letter-spacing: 0;
}
.coupon-number .p {
  display: flex;
}
.coupon-number .p .text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #151500;
  letter-spacing: 0;
}
.coupon-number .p .text text {
  color: #FEB21C ;
}
.coupon-number .p .icon {
  margin-left: 24rpx;
  margin-right: 24rpx;
  color: #999999;
}

.goods-info {
  margin-top: 24rpx;
  padding: 36rpx 24rpx;
}
.goods-info .item {
  width: 702rpx;
  margin-top: 32rpx;
  display: flex;
  justify-content:space-between;
}
.goods-info .item .t {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 28rpx;
}
.goods-info .item .v {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: right;
  line-height: 28rpx;
}
.goods-info .item .sft {
  color: #333333;
}
.goods-info .item .sfv {
  color: #FEB21C;
}

.jiesuan-box{
  display: flex;
  justify-content: space-between;
  background: #333333;
  width: 750rpx;
  height: 98rpx;
